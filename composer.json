{
    "name": "ndestates/laravel-model-schema-checker",
    "description": "A comprehensive Laravel application validator for security, performance, relationships, migrations, and code quality",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "NDEstates",
            "email": "support@ndestates.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "NDEstates\\LaravelModelSchemaChecker\\": "src/"
        }
    },
    "bin": [
        "bin/laravel-model-schema-checker"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "NDEstates\\LaravelModelSchemaChecker\\ModelSchemaCheckerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}