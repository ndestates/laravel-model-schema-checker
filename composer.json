{
    "name": "ndestates/laravel-model-schema-checker",
    "description": "A comprehensive Laravel tool for validating model fillable properties against database schema and checking Filament relationship integrity",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "NDEstates",
            "email": "support@ndestates.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NDEstates\\LaravelModelSchemaChecker\\": "src/",
            "Check\\": "check/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NDEstates\\LaravelModelSchemaChecker\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
            "chmod +x run-checker.sh"
        ],
        "post-update-cmd": [
            "chmod +x run-checker.sh"
        ]
    },
    "bin": [
        "run-checker.sh"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "NDEstates\\LaravelModelSchemaChecker\\ModelSchemaCheckerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}